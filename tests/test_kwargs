set_atters = {
    'keyword1': 'foo',
    'keyword2': 'bar',
    'keyword3': 'fish',
}


class Test:
    def __init__(self, set_attrs):

        self.setattrs(**set_attrs)

        self.print_attrs()

    def setattrs(self, **kwargs):
        for k, v in kwargs.items():
            setattr(self, k, v)

    def print_attrs(self):
        print(f"{self.keyword1=}")
        print(f"{self.keyword2=}")
        print(f"{self.keyword3=}")


test = Test(set_atters)
